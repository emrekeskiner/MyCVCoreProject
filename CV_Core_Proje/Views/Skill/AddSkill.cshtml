@{
    ViewData["Title"] = "AddSkill";
    Layout = "~/Views/Shared/_NewAdminLayout.cshtml";
}
<div class="card">
    <div class="card-body">
        <h4 class="card-title">Yetenek Ekle</h4>
    <form method="post">

        <div class="form-group">
            <label class="form-control-label" for="skillName">Başlık</label>
            <input type="text" id="skillName" class="form-control" name="Title" >
        </div>
        <div class="form-group">
            <label class="form-control-label" for="oran">Oran</label>
            <input type="text" id="oran" class="form-control" name="Value" >
        </div>
            <button class="btn btn-inverse-primary btn-fw">Ekle</button>
    </form>
    </div>
</div>